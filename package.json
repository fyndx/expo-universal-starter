{
  "name": "universal-starter",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "bun run --filter './apps/*' dev",
    "android": "bun run --filter './apps/frontend' android",
    "ios": "bun run --filter './apps/frontend' ios",
    "web": "bun run --filter './apps/frontend' web"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
